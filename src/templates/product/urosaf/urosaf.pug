include ../../mixins/product-page

-
	var PRODUCT_NAME = 'Urosaf'
	var PRODUCT_CARD = LANG.product_card;
	var CURRENT_PRODUCT = LANG.urosaf;
	var RECOMMENDATIONS = LANG.recommendations.filter(prod => prod.name === 'Deluron Forte' || prod.name === 'Paraxan' || prod.name === 'Steplex')
	var REVIEWS = LANG.reviews.reviews_urosaf;
	REVIEWS.length = 3;

+productPage(PRODUCT_NAME, PRODUCT_CARD, CURRENT_PRODUCT, RECOMMENDATIONS, REVIEWS)
