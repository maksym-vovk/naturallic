include ../../mixins/product-page

-
	var PRODUCT_NAME = 'Desalix'
	var PRODUCT_CARD = LANG.product_card;
	var CURRENT_PRODUCT = LANG.desalix;
	var RECOMMENDATIONS = LANG.recommendations.filter(prod => prod.name === 'Steplex' || prod.name === 'Keramin' || prod.name === 'Paraxan')
	var REVIEWS = LANG.reviews.reviews_desalix;
	REVIEWS.length = 3;

+productPage(PRODUCT_NAME, PRODUCT_CARD, CURRENT_PRODUCT, RECOMMENDATIONS, REVIEWS)