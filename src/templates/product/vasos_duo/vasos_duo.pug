include ../../mixins/product-page

-
    var PRODUCT_NAME = 'Vasos Duo'
    var PRODUCT_CARD = LANG.product_card;
    var CURRENT_PRODUCT = LANG.vasos_duo;
    var RECOMMENDATIONS = LANG.recommendations.filter(prod => prod.name === 'Deluron Forte' || prod.name === 'Urosaf' || prod.name === 'Steplex')
    var REVIEWS = LANG.reviews.reviews_vasos_duo;
    REVIEWS.length = 3;

+productPage(PRODUCT_NAME, PRODUCT_CARD, CURRENT_PRODUCT, RECOMMENDATIONS, REVIEWS)
